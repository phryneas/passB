{
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "manifest_version": 2,
  "version": "1.0.0",
  "homepage_url": "https://passb.github.io",
  "applications": {
    "gecko": {
      "id": "passb@phryneas.de",
      "strict_min_version": "45.0"
    }
  },
  "browser_action": {
    "browser_style": false,
    "default_popup": "popup.html",
    "default_icon": {
      "16": "passB_16.png",
      "32": "passB_32.png"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "background": {
    "scripts": [
      "built/shared.js",
      "built/background-script.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      }
    }
  },
  "permissions": ["nativeMessaging", "tabs", "activeTab", "storage", "unlimitedStorage", "clipboardWrite"]
}
